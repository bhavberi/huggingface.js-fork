{
	"name": "@huggingface/templates",
	"packageManager": "pnpm@8.10.5",
	"version": "0.0.1",
	"description": "A minimalistic JavaScript implementation of the Jinja templating engine, specifically designed for parsing ML chat templates.",
	"repository": "https://github.com/huggingface/huggingface.js.git",
	"publishConfig": {
		"access": "public"
	},
	"main": "index.ts",
	"type": "module",
	"browser": {
		"./dist/index.js": "./dist/browser/index.js"
	},
	"engines": {
		"node": ">=18"
	},
	"source": "index.ts",
	"scripts": {
		"lint": "eslint --quiet --fix --ext .cjs,.ts .",
		"lint:check": "eslint --ext .cjs,.ts .",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"prepublishOnly": "pnpm run build",
		"build": "tsup",
		"test": "vitest run",
		"test:browser": "vitest run --browser.name=chrome --browser.headless",
		"check": "tsc"
	},
	"files": [
		"src",
		"dist",
		"index.ts"
	],
	"keywords": [
		"huggingface",
		"jinja",
		"templates",
		"hugging",
		"face"
	],
	"author": "Hugging Face",
	"license": "MIT",
	"devDependencies": {
		"@xenova/transformers": "^2.9.0",
		"typescript": "^5.3.2",
		"vitest": "^0.34.6"
	}
}
